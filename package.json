    "bugs": {
        "url": "https://github.com/disjfa/x-brewing/issues"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.6.3",
        "@symfony/webpack-encore": "^0.22.4",
        "autoprefixer": "^9.4.6",
        "bootstrap": "^4.2.1",
        "fancy-log": "^1.3.3",
        "favicons": "^5.3.0",
        "gh-pages": "^2.0.1",
        "gulp": "^4.0.0",
        "gulp-util": "^3.0.8",
        "leaflet": "^1.4.0",
        "lodash": "^4.17.11",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
