{
    "name": "x-brewing",
    "version": "0.1.0",
    "description": "Homepage",
    "homepage": "https://github.com/disjfa/x-brewing#readme",
    "keywords": [
        "css",
        "sass",
        "mobile-first",
        "responsive",
        "front-end",
        "web",
        "beer",
        "brewery"
    ],
    "scripts": {
        "build": "encore production && bundle exec jekyll build && encore production && bundle exec jekyll build",
        "deploy": "gh-pages -d _site",
        "dev": "encore dev --watch",
        "favicon": "gulp favicon",
        "generate": "all-contributors generate",
        "jekyll-build": "bundle exec jekyll build",
        "jekyll-serve": "bundle exec jekyll serve",
        "prod": "encore production",
        "serve": "npm-run-all --parallel dev jekyll-serve"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/disjfa/x-brewing.git"
    },
